    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.1",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.2",
    "vite": "^5.0.0",
