    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "lucide-vue-next": "^0.294.0",
    "monaco-editor": "^0.44.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
